#selectorBiz2Product.modal.modal-wide.hide.fade
  .modal-header
    %nav
      %a.widgetbtn{"data-toggle" => "modal", :href => "#comingsoonModal"}
        %i.gcmsicon-questionmark-grey
      %a.widgetbtn{"data-dismiss" => "modal", :href => "#"}
        %i.gcmsicon-x-grey
    %h1
      Link %Link-to-type% to link to this Product
      %span.version v 0.32
  .modal-body
    .alert.alert-info
      %span#alertMessage
      %button.close{:type => "button", :"data-dismiss" => "alert"} Ã—
    %form
      .row-fluid
        .span6

          %h4
            Select %Link-to-type% to link
            .WidgetBoxHeading.pull-right
              %input{:type => "text", :placeholder => "Search %Link-to-type%s"}
          
          .filter-block
          
            .row-fluid
              .tabbable.tabs-left
                -#%ul.nav.nav-tabs
                  %li.active 
                    %a{:href => "#bizobj1", "data-toggle" => "tab"}
                      Biz Object 1
                  %li 
                    %a{:href => "#bizobj2", "data-toggle" => "tab"}
                      Biz Object 2
                .tab-content
                  #bizobj1.tab-pane.active
                    #currentList1.system-list
                      %ul.source
                        %li
                          .row-fluid
                            .span8
                              %span.system System Company
                              %span.name System 1
                            .span4.actions                
                              %a.widgetbtn.pull-right.add-me{:href => "#"}
                                %i.grcicon-chevron-right
                              %a.widgetbtn.pull-right{:href => "#"}
                                %i.gcmsicon-edit-grey
                        %li
                          .row-fluid
                            .span8
                              %span.system System Company
                              %span.name System 2
                            .span4.actions              
                              %a.widgetbtn.pull-right.add-me{:href => "#"}
                                %i.grcicon-chevron-right 
                              %a.widgetbtn.pull-right{:href => "#"}
                                %i.gcmsicon-edit-grey
                        %li
                          .row-fluid
                            .span8
                              %span.system System Company
                              %span.name System 3
                            .span4.actions       
                              %a.widgetbtn.pull-right.add-me{:href => "#"}
                                %i.grcicon-chevron-right
                              %a.widgetbtn.pull-right{:href => "#"}
                                %i.gcmsicon-edit-grey
                  #bizobj2.tab-pane
                    #currentList2.system-list
                      %ul.source
                        %li
                          .row-fluid
                            .span8
                              %span.system System Company
                              %span.name System 1
                            .span4.actions                
                              %a.widgetbtn.pull-right.add-me{:href => "#"}
                                %i.grcicon-chevron-right
                              %a.widgetbtn.pull-right{:href => "#"}
                                %i.gcmsicon-edit-grey
        .span6
          %h4 %Link-to-type%s linked to this Product
          .system-list.currentlist
            %ul.target
              %li
                .row-fluid
                  .span6  
                    %span.system Sub-System Company
                    %span.name System 1
                  .span6.actions
                    %a#removeMe.widgetbtn.pull-right{:href => "#"}
                      %i.icon-minus-sign
                    %a.widgetbtn.pull-right{"data-toggle" => "modal", :href => "#redesignNewCategory"}
                      %i.gcmsicon-edit-grey 
              %li
                .row-fluid
                  .span6
                    %span.system System Company
                    %span.name System 2
                  .span6.actions
                    %a#removeMe.widgetbtn.pull-right{:href => "#"}
                      %i.icon-minus-sign
                    %a.widgetbtn.pull-right{"data-toggle" => "modal", :href => "#redesignNewCategory"}
                      %i.gcmsicon-edit-grey 
              %li
                .row-fluid
                  .span6
                    %span.system System Company
                    %span.name System 3
                  .span6.actions
                    %a#removeMe.widgetbtn.pull-right{:href => "#"}
                      %i.icon-minus-sign
                    %a.widgetbtn.pull-right{"data-toggle" => "modal", :href => "#redesignNewCategory"}
                      %i.gcmsicon-edit-grey 
              
  =render :partial => 'design/templates/modalfooter', :locals => { :shownote => false, :showcreated => false, :showrevs => false }